lock></next></block></next></block></next></block></statement></block></xml>","main.ts":"// Animación del dado\nfunction rollDiceAnimation () {\n    for (let index = 0; index < 3; index++) {\n        basic.showLeds(`\n            . . # . .\n            . . . . .\n            . . # . .\n            . . . . .\n            . . # . .\n            `)\n        basic.pause(100)\n        basic.showLeds(`\n            . . . . .\n            . # . # .\n            . . . . .\n            . # . # .\n            . . . . .\n            `)\n        basic.pause(100)\n        basic.showLeds(`\n            # . . . #\n            . . . . .\n            . . # . .\n            . . . . .\n            # . . . #\n            `)\n        basic.pause(100)\n    }\n}\n// Animación para serpientes\nfunction showSnake () {\n    music.playTone(330, 200)\n    basic.showLeds(`\n        . . # # .\n        . # . . .\n        . . # . .\n        . . . # .\n        . # # # .\n        `)\n    basic.pause(500)\n}\n// Animación para escaleras\nfunction showLadder () {\n    music.playTone(262, 200)\n    basic.showLeds(`\n        # . . . #\n        # # # # #\n        # . . . #\n        # # # # #\n        # . . . #\n        `)\n    basic.pause(500)\n}\n// Turno del jugador 1 (Botón A)\ninput.onButtonPressed(Button.A, function () {\n    if (currentPlayer == 1) {\n        rollDiceAnimation()\n        // Lanza el dado (1-6)\n        diceRoll = Math.randomRange(1, 6)\n        music.playTone(392, 200)\n        // Avanza el jugador 1\n        player1Position += diceRoll\n        if (player1Position > 15) {\n            // No puede pasar de la meta\n            player1Position = 15\n        }\n        if (board[player1Position] > player1Position) {\n            // Subir por una escalera\n            showLadder()\n        } else if (board[player1Position] < player1Position) {\n            // Bajar por una serpiente\n            showSnake()\n        }\n        // Aplica escaleras o serpientes\n        player1Position = board[player1Position]\n        // Muestra las posiciones\n        showPositions()\n        if (player1Position == 15) {\n            music.playMelody(\"C5 B A G F E D C \", 120)\n            basic.showString(\"P1 WINS!\")\n            return\n        }\n        // Cambia el turno al jugador 2\n        currentPlayer = 2\n    }\n})\n// Mostrar las posiciones de ambos jugadores\nfunction showPositions () {\n    basic.clearScreen()\n    x1 = player1Position % 5\n    y1 = Math.floor(player1Position / 5)\n    // Jugador 1 (brillo medio)\n    led.plotBrightness(x1, y1, 150)\n    x2 = player2Position % 5\n    y2 = Math.floor(player2Position / 5)\n    // Jugador 2 (brillo máximo)\n    led.plotBrightness(x2, y2, 255)\n}\n// Turno del jugador 2 (Botón B)\ninput.onButtonPressed(Button.B, function () {\n    if (currentPlayer == 2) {\n        rollDiceAnimation()\n        // Lanza el dado (1-6)\n        diceRoll = Math.randomRange(1, 6)\n        music.playTone(392, 200)\n        // Avanza el jugador 2\n        player2Position += diceRoll\n        if (player2Position > 15) {\n            // No puede pasar de la meta\n            player2Position = 15\n        }\n        if (board[player2Position] > player2Position) {\n            // Subir por una escalera\n            showLadder()\n        } else if (board[player2Position] < player2Position) {\n            // Bajar por una serpiente\n            showSnake()\n        }\n        // Aplica escaleras o serpientes\n        player2Position = board[player2Position]\n        // Muestra las posiciones\n        showPositions()\n        if (player2Position == 15) {\n            music.playMelody(\"C5 B A G F E D C \", 120)\n            basic.showString(\"P2 WINS!\")\n            return\n        }\n        // Cambia el turno al jugador 1\n        currentPlayer = 1\n    }\n})\n// Reiniciar el juego\ninput.onGesture(Gesture.Shake, function () {\n    player1Position = 0\n    player2Position = 0\n    currentPlayer = 1\n    showPositions()\n    music.playMelody(\"C D E F G A B C5 \", 120)\n    basic.showString(\"RESET\")\n})\nlet y2 = 0\nlet player2Position = 0\nlet x2 = 0\nlet y1 = 0\nlet x1 = 0\nlet player1Position = 0\nlet diceRoll = 0\nlet board: number[] = []\nlet currentPlayer = 0\n// Indica qué jugador tiene el turno\ncurrentPlayer = 1\n// Configuración del tablero con escaleras y serpientes\n// Casilla 0 (inicio)\n// Casilla 1\n// Casilla 2\n// Casilla 3\n// Escalera: sube de 4 a 8\n// Casilla 5\n// Casilla 6\n// Serpiente: baja de 7 a 2\n// Casilla 8\n// Casilla 9\n// Casilla 10\n// Serpiente: baja de 11 a 3\n// Casilla 12\n// Casilla 13\n// Casilla 14\n// Casilla 15 (meta)\nboard = [\n0,\n1,\n2,\n3,\n8,\n5,\n6,\n2,\n8,\n9,\n10,\n3,\n12,\n13,\n14,\n15\n]\n// Mostrar las posiciones iniciales\nshowPositions()\n","README.md":"","pxt.json":"{\n    \"name\": \"primera clase\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}}],"shares":[],"lastSaveTime":1737695055975}